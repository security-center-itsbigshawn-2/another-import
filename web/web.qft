    <Package id="_1j+i" name="EDGE">
      <Procedure id="_1j+p" name="handleDownloadDialog">
        <variable name="filename"/>
        <variable name="title">(?i)Datei.*auswählen|Datei hochladen|.*file upload.*</variable>
        <variable name="button">(?i).*speichern|.*save</variable>
        <comment>Handle the native Download dialog

@param	filename	The name of the file to be downloaded
@param	title	A regexp for the title of the dialog.
@param	button	A regexp for the text on the save button.

@author	QFS, pb</comment>
        <IfSequence id="_1j+q" test="${qftest:windows}">
          <ServerScriptStep id="_1j+r">
            <code>import autowin
import time
from java.io import File

fileName = File(rc.lookup("filename")).getCanonicalPath()

dialogTitleRe = rc.lookup("title")
btnTitleRe = rc.lookup("button")

wnd = autowin.waitForWindow(5000, dialogTitleRe, 1)
if not wnd:
    raise UserException("The dialog did not appear.")
wnd.setForeground()

edits = autowin.findChildWindows(wnd, clazz="Edit")
if len(edits) == 0:
    raise UserException("Window not found.")
autowin.setWindowText(edits[0], fileName)
time.sleep(1)

btns = autowin.findChildWindows(wnd, btnTitleRe, 1, "Button")
if len(btns) == 0:
    rc.logWarning("Button '%s' not found" %btnTitleRe)
    btns = autowin.findChildWindows(wnd, None, 0, "Button")
    if len(btns) == 0:
        raise UserException("Button not found.")
btn = btns[0]
autowin.doClick(btn, 10, 5)
</code>
          </ServerScriptStep>
        </IfSequence>
      </Procedure>
      <Procedure id="_1j+j" name="download">
        <variable name="componentToClick">linkDownload</variable>
        <variable name="filename">a name for the file</variable>
        <TryStep id="_1j+k">
          <MouseEventStep clicks="1" client="$(client)"
                          component="$(componentToClick)" event="MOUSE_MPRC"
                          id="_1j+c" modifiers="16"/>
          <ProcedureCall id="_1j+d" procedure="qfs.autowin.component.click">
            <variable name="window">.*Microsoft Edge.*</variable>
            <variable name="name"/>
            <variable name="automationId">m_chevronButton</variable>
            <variable name="controlType"/>
            <variable name="index"/>
            <variable name="className"/>
            <variable name="timeout">10000</variable>
          </ProcedureCall>
          <ProcedureCall id="_W" predelay="200"
                         procedure="qfs.autowin.keyevents.pressEnter">
            <variable name="title">.*Microsoft Edge.*</variable>
            <variable name="titleRegexp">true</variable>
          </ProcedureCall>
          <ProcedureCall id="_1j+e" procedure="EDGE.handleDownloadDialog">
            <variable name="filename">$(filename)</variable>
            <variable name="title">(?i).*Speichern unter.*|.*Datei speichern|.*file upload.*|.*download file.*</variable>
            <variable name="button">(?i).*speichern|.*save</variable>
          </ProcedureCall>
