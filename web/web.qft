    <DependencyReference id="_1j-m" reference="Options and Startup"/>
    <TestCase id="_1" name="Download automatically - Firefox">
      <variable name="browser">firefox</variable>
      <MouseEventStep clicks="1" client="$(client)" component="linkDownload"
                      event="MOUSE_MPRC" id="_r" modifiers="16"/>
      <CommentStep id="_u">
        <heading>You have to wait for the download to finish.</heading>
        <comment>Maybe create a script to pool the file system.</comment>
      </CommentStep>
      <CommentStep id="_1m" postdelay="2000">
        <heading>For now 2000 ms harcoded delay. The file is rather small.</heading>
      </CommentStep>
      <ProcedureCall id="_s" procedure="qfs.shellutils.exists">
        <variable name="file">$(downloadFolder)/7z1900.exe</variable>
      </ProcedureCall>
    </TestCase>
    <TestCase id="_w" name="Download - Chrome">
      <variable name="browser">chrome</variable>
      <SetGlobalStep id="_1j-5" varname="file">
        <default>$(downloadFolder)/7zip.chrome.exe</default>
      </SetGlobalStep>
      <ProcedureCall id="_1q" procedure="Chrome.download">
        <variable name="componentToClick">linkDownload</variable>
        <variable name="filename">$(file)</variable>
      </ProcedureCall>
      <CommentStep id="_1j+x">
        <heading>You have to wait for the download to finish.</heading>
        <comment>Maybe create a script to pool the file system.</comment>
      </CommentStep>
      <CommentStep id="_1j+y" postdelay="2000">
        <heading>For now 2000 ms harcoded delay. The file is rather small.</heading>
      </CommentStep>
      <ProcedureCall id="_1j+z" procedure="qfs.shellutils.exists">
        <variable name="file">$(file)</variable>
      </ProcedureCall>
    </TestCase>
    <TestCase id="_1j+J" name="Download - Edge">
      <variable name="browser">edge</variable>
      <SetGlobalStep id="_1j-c" varname="file">
        <default>$(downloadFolder)/7zip.edge.exe</default>
      </SetGlobalStep>
      <ProcedureCall id="_1j-b" procedure="EDGE.download">
        <variable name="componentToClick">linkDownload</variable>
        <variable name="filename">$(file)</variable>
      </ProcedureCall>
      <ProcedureCall id="_1j-k" procedure="qfs.shellutils.exists">
        <variable name="file">$(file)</variable>
      </ProcedureCall>
